/**index.wxss**/
.container{margin-top:0px;}
.container button{width:99%;margin-top:5px;}

.swiper{ width: 100%; position: relative; padding-bottom: 50%;}
.swiper swiper{ position: absolute; left: 0; top: 0;  width: 100%; height: 100%;}
.swiper image{ width: 100%; height: 100%;}
.swiper .dots{position: absolute;left:0;right: 0;bottom: 20rpx;display: flex;justify-content: center;}
.swiper .dots .dot{margin: 0 8rpx;width: 16rpx;height: 16rpx;border-radius: 8rpx;transition: all .0s;}
.swiper .dots .dot.active{width: 30rpx;background: #096CEB;}

 /*nav*/
.nav{
 flex-wrap:wrap;
 justify-content:center;
 margin-top: 10px;
}
.nav-item{
  margin: 20rpx 10rpx 10rpx 10rpx;
  width:355rpx;
  background: #096CEB;
  float: left;
   border-radius:5rpx;
}
.nav-item image{
  width:80rpx;
  height: 60rpx;
  margin: 20rpx;float: left;
}
.nav-item text{
  width:160rpx;
  font-size: 26rpx;
  line-height: 95rpx;
  color:#fff;
  display: inline-block;
  text-align: center;

}
.case_title{
  width: 100%;
    height: 100%;
    display: flex;
    justify-content: center;
}
.case_title text{ color: #096CEB;  font-size: 26rpx;
font-weight: bold;margin: 20px;  
} 
.caselist {
    width:100%;
    height: 100%;
    display: flex;
    justify-content: center;
}
.caselist ul{
  width: 95%;
  margin: 0 auto;
}
.caselist ul li{
  margin: 20rpx 10rpx 10rpx 10rpx;
  width:355rpx;

 }
.caselist image{
    width: 100%;
    height: 140rpx;
  padding: 15rpx;
}
.caselist text{ color: #096CEB;  font-size: 26rpx;
font-weight: bold;margin: 10px auto;  
 justify-content: center;
 display: flex;
} 
.part{
  width: 95%;
  margin: 0 auto;
  clear: both;
}
.part-title {
  background-color: #fff;
  height: 92rpx;
  padding-left: 20rpx;
  color: #333;text-align: center;
  font-size: 36rpx;
  line-height: 92rpx;
}

.part .recommend-wrapper {
  box-sizing: border-box;
  width: 100%;
  padding: 0 20rpx;
  position: relative;
  overflow: hidden;
  background-color: #fff;
}

/*.part .recommend {
  width: 1000000rpx;
}*/

.part .recommend-wrapper .recommend-item {
  /*float: left;*/
  width: 600rpx;
  background-color: #fff;
}

.part .recommend-wrapper .recommend-item image{
  width: 100%;
}

.part .recommend-wrapper .recommend-item a {
  display: block;
  width: 600rpx;
  height: 378rpx;
  background-color: #ff7200;
  background-size: cover;
}

.part .recommend-wrapper .recommend-item-title {
  height: 90rpx;
  color: #464646;
  font-size: 32rpx;
  line-height: 80rpx;
  margin-bottom: 10rpx;
}

.extra-part {
  background-color: #fff;
  height: 140rpx;
  line-height: 140rpx;
  text-align: center;
}

.extra-part a {
  font-size: 36rpx;
  color: #ff7200;
}

.place-item {
  display: block;
  width: 100%;
  padding-bottom: 20rpx;
  background-color: #efefef;
}

.place-item-img {
  position: relative;
  height: 470rpx;
  background-color: #e2e1e1;
  background-repeat: no-repeat;
  background-position: center center;
  background-size: cover;
}

.place-item-img .corner-info,
.place-item-img .placeType-info,
.place-item-img .collection {
  position: absolute;
  display: block;
}

.place-item-img .corner-info {
  top: 0;
  left: 0;
  width: 94rpx;
  height: 94rpx;
}

.place-item-img .corner-info img {
  width: 100%;
}

.place-item-img .placeType-info {
  bottom: 20rpx;
  left: 20rpx;
}

.place-item-img .placeType-info i {
  display: inline-block;
  margin-right: 12rpx;
  min-height: 32rpx;
  padding: 10rpx 12rpx;
  background-color: rgba(0, 0, 0, 0.5);
  border-radius: 5rpx;
  font-size: 24rpx;
  color: #fff;
}

.place-item-info {
  padding: 12rpx 20rpx;
  background-color: #fff;
}

/*.place-item-info .place-item-info-title {
  line-height: 58rpx;
  font-size: 32rpx;
  color: #464646;
}*/

.place-item-info .place-item-info-title {
  float: left;
  width: 460rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.place-item-info .place-item-info-price {
  float: right;
  font-size: 34rpx;
  color: #ff7200;
}

.place-item-info .place-item-info-price i {
  font-size: 24rpx;
}

.place-item-info .place-item-info-detail {
  line-height: 36rpx;
  font-size: 26rpx;
  color: #878787;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.place-item-info .place-item-info-detail i {
  margin-right: 14rpx;
}