/* contact.wxss */
@import '../../style/weui.wxss';
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding:0;
  box-sizing: border-box;
} 
.banner{margin: 0;padding: 0;width: 100%;}
image{width: 100%;height: 120px;}
.page .page__bd{width:90%;margin: 50px auto;text-align: center;}
.contact-title {
  background-color: #fff;
  height: 92rpx;
  padding-left: 20rpx;
  color: #333;text-align: center;
  font-size: 36rpx;
  line-height: 92rpx;
}

.contact{
  margin: 0 auto;
  width: 90%;
}
.lin{height: 30px;}